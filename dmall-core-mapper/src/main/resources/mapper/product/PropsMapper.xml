<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dmall.product.mapper.PropsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dmall.product.entity.Props">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="product_type" property="productType" />
        <result column="group_id" property="groupId" />
        <result column="input_mode" property="inputMode" />
        <result column="input_type" property="inputType" />
        <result column="validate_pattern" property="validatePattern" />
        <result column="remark" property="remark" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, product_type,group_id, input_mode, input_type, validate_pattern, remark, create_time, update_time
    </sql>

</mapper>
